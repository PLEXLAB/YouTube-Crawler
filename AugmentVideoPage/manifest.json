{
	"name": "Change video Page",
	"version": "0.0.2",
	"manifest_version": 2,
	"description": "Automate crawling youtube channel crawling",
	"homepage_url": "https://github.com/PLEXLAB/...",
	"icons": {
		"16": "icons/plex16.png",
		"19": "icons/plex19.png",
		"48": "icons/plex48.png",
		"128": "icons/plex128.png"
	},
	"background":{
		"scripts": [
			"src/inject/socket.io.js", 
			"src/inject/jquery-3.3.1.min.js",
			"src/background/background.js"
		]
	},
	"web_accessible_resources": ["src/inject/images/demonitizationSymbol.jpg"],
	"permissions": [
		"https://youtube.com/*",
		"tabs",
		"activeTab",
		"background", 
		"storage"
	],
	"content_scripts": [{
		"matches": ["https://www.youtube.com/watch?v=*", "http://www.youtube.com/watch?v=*"],
		"js": [
			"src/inject/socket.io.js", 
			"src/inject/jquery-3.3.1.min.js", 
			"src/inject/inject.js"			
		]
    }]
}